<div class="panel panel-default">
  <div class="panel-heading">
    <h4>インポート</h4>
  </div>

  <div class="panel-body">
    <%= render 'form' %>

    <% if @error_row.present? %>
      <p class="text-danger"><%= @error_row %>行目でエラーが発生しました</p>
    <% end %>
  </div>
</div>

<%= link_to t('helpers.link.back'), bots_path, class: "btn btn-default", data: { event_name: 'Back to bot index from imports', bot_id: @bot.id, bot_name: @bot.name } %>
