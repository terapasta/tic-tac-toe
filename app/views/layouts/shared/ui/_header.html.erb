<header>
  <nav class="navbar navbar-default navbar-fixed-top">
    <div class="container">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <%= link_to 'My-ope office', root_path, class: "navbar-brand" %>
      </div>
      <div id="navbar" class="navbar-collapse collapse">
        <ul class="nav navbar-nav">
          <!--
          <li class="active"><a href="#">Home</a></li>
          <li><a href="#about">About</a></li>
          <li><a href="#contact">Contact</a></li>
        -->
        </ul>
        <% if user_signed_in? %>
          <ul class="nav navbar-nav navbar-right">
            <li><%= link_to "ヘルプ", " https://mofmof.zendesk.com", target: "blank" %></li>
            <li><%= link_to "お問い合わせ", new_contacts_path %></li>
            <% if policy(:bot).index? %>
              <li><%= link_to "Bot一覧", bots_path, data: { event_name: 'Open bot index', bot_id: @bot.try(:id), bot_name: @bot.try(:name) } %></li>
            <% end %>
            <% if current_user.staff? %>
              <li><%= link_to "システム共通同義語一覧", admin_word_mappings_path, data: { event_name: 'Open admin word mapping index', bot_id: @bot.try(:id), bot_name: @bot.try(:name) } %></li>
            <% end %>
            <% if policy(:word_mapping).index? %>
              <li><%= link_to "同義語一覧", word_mappings_path, data: { event_name: 'Open word mapping index', bot_id: @bot.try(:id), bot_name: @bot.try(:name) } %></li>
            <% end %>
            <% if policy(current_user).edit? %>
              <li><%= link_to 'ユーザー情報変更', edit_user_registration_path, data: { event_name: 'Open edit user', bot_id: @bot.try(:id), bot_name: @bot.try(:name) } %></li>
            <% end %>
            <li><%#= link_to "管理者画面", rails_admin_path %></li>
            <!--<li class="dropdown">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">マスタ&nbsp;<span class="caret"></span></a>
              <ul class="dropdown-menu">
                <li><%#= link_to '回答', rails_admin.index_path(model_name: :answer) %></li>
                <li><%#= link_to '文脈', rails_admin.index_path(model_name: :context) %></li>
              </ul>
            </li>-->
            <li><%= link_to "ログアウト", destroy_user_session_path, data: { method: :delete, event_name: 'Logout', bot_id: @bot.try(:id), bot_name: @bot.try(:name) } %></li>
          </ul>
        <% end %>
      </div><!--/.nav-collapse -->
    </div>
  </nav>
</header>
