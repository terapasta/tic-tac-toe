{
  "name": "donusagi-bot",
  "description": "[My-ope office](http://www.my-ope.net/)という社内問合せ特化のAIチャットBotサービスのリポジトリです。",
  "scripts": {
    "watch": "NODE_ENV=development watchify app/javascripts/all.js -o app/assets/javascripts/bundled-es2015.js --extension=.jsx -v -t [ babelify --presets [ es2015 react ] --plugins [ transform-inline-environment-variables ] ]",
    "watch-embed": "NODE_ENV=development watchify app/javascripts/embed.js -o app/assets/javascripts/embed.js --extension=.jsx -v -t [ babelify --presets [ es2015 react ] --plugins [ transform-inline-environment-variables ] ]",
    "build": "NODE_ENV=production browserify app/javascripts/all.js --extension=.jsx -o app/assets/javascripts/bundled-es2015.js -v -t [ babelify --presets [ es2015 react ] --plugins [ transform-inline-environment-variables ] ]",
    "build-embed": "NODE_ENV=production browserify app/javascripts/embed.js --extension=.jsx -o app/assets/javascripts/embed.js -v -t [ babelify --presets [ es2015 react ] --plugins [ transform-inline-environment-variables ] ]",
    "test": "./node_modules/.bin/jest"
  },
  "jest": {
    "verbose": true,
    "automock": true,
    "roots": [
      "./spec/javascripts",
      "./app/assets/javascripts/es2015"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "testPathIgnorePatterns": [
      "test-helper"
    ],
    "unmockedModulePathPatterns": [
      "test-helper",
      "lodash",
      "react",
      "enzyme",
      "axios",
      "is-empty",
      "react-textarea-autosize",
      "classnames",
      "sinon",
      "promise",
      "redux-actions",
      "redux-promise",
      "uuid"
    ]
  },
  "dependencies": {
    "@rails/webpacker": "^3.0.2",
    "axios": "^0.15.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-inline-environment-variables": "^0.0.2",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "bytes": "^2.5.0",
    "c3": "^0.5.1",
    "classnames": "^2.2.5",
    "coffeescript": "1.12.7",
    "copy-to-clipboard": "^3.0.6",
    "core-js": "^2.5.3",
    "enzyme": "^2.7.1",
    "es6-promise": "^4.1.1",
    "formik": "^0.9.2",
    "imagesloaded": "^4.1.3",
    "is-empty": "^1.0.0",
    "jest-cli": "^19.0.2",
    "joi-browser": "^13.0.1",
    "jquery": "^3.1.1",
    "js-cookie": "^2.1.4",
    "json2csv": "^3.7.3",
    "lodash": "^4.17.2",
    "markdown": "^0.5.0",
    "object-to-formdata": "^1.0.12",
    "promise": "^7.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^16.2.0",
    "react-highlight-words": "^0.8.1",
    "react-linkify": "^0.2.0",
    "react-loading": "^1.0.3",
    "react-nl2br": "^0.2.0",
    "react-radio-group": "^3.0.1",
    "react-redux": "^5.0.6",
    "react-textarea-autosize": "^5.2.1",
    "redux": "^3.7.2",
    "redux-actions": "^2.0.1",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "sanitize-html": "^1.17.0",
    "sinon": "^1.17.7",
    "striptags": "^3.1.1",
    "styled-components": "^2.1.1",
    "toastr": "^2.1.2",
    "uuid": "^3.0.1",
    "vue": "^2.5.13",
    "vue-loader": "^13.6.1",
    "vue-multiselect": "^2.0.8",
    "vue-router": "^3.0.1",
    "vue-sweetalert": "^0.1.18",
    "vue-template-compiler": "^2.5.13",
    "vuex": "^3.0.1",
    "watchify": "^3.7.0",
    "yup": "^0.22.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.2.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "webpack-dev-server": "^2.9.2"
  }
}
